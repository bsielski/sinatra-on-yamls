<div>
    <a href="/tasks/new">New Task</a>
    <table>
	<tr>
	    <th>id</th>
	    <th>data</th>
	    <th></th>
	</tr>
	<% model.each do |item| %>
	    <tr>
		<td><%= item[:id] %></td>
		<td><%= item[:data] %></td>
		<td>
		    <a href=<%= delete_href(item[:id]) %>>
			Delete
		    </a>
		</td>
	    </tr>
	<% end %>
    </table>
</div>
